<section class="hero">
    <div class="hero-inner">
        <h2>Welcome to...</h2>
        <h1>Tech</h1>
        <h1 id="logo-orange">Ready</h1>
        <h2><span id="typewriter"></span><span id="cursor"> </span></h2>
    </div>
</section>

<section>
    <ul>
        <li>Trending</li>
        <li>Recent</li>
    </ul>

    {{#each question as |question| }}
        <a href="/question/{{id}}">
            <div class="card">
                <h3 class="question_title">{{question_title}}</h3>
                <p>{{format_date date_created}}</p>
            </div>
        </a>
    {{/each}}



</section>







<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="js/typewriter.js"></script>